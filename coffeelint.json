{
  "arrow_spacing": {
    "level": "error"
  },

  "braces_spacing": {
    "empty_object_spaces": 0,
    "spaces": 0,
    "level": "error"
  },

  "colon_assignment_spacing": {
    "spacing": {
      "left": 0,
      "right": 1
    },
    "level": "error"
  },

  "line_endings": {
    "value": "unix",
    "level": "error"
  },

  "max_line_length": {
    "name": "max_line_length",
    "value": 100,
    "level": "error",
    "limitComments": true
  },

  "no_empty_param_list": {
    "level": "error"
  },

  "no_interpolation_in_single_quotes": {
    "level": "error"
  },

  "no_stand_alone_at": {
    "level": "error"
  },

  "no_this": {
    "level": "error"
  },

  "space_operators": {
    "level": "error"
  },

  "spacing_after_comma": {
    "level": "error"
  }
}
